{
  "name": "ping_tunnel_edge",
  "version": "0.0.4",
  "description": "Edge client for QUIC tunnel",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "quic",
    "tunnel",
    "edge",
    "network",
    "native",
    "napi"
  ],
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "napi": {
    "name": "ping_tunnel",
    "triples": {
      "defaults": true,
      "additional": []
    }
  },
  "scripts": {
    "build": "napi build --platform --release --features napi -o ./build",
    "build:debug": "napi build --platform --features napi",
    "build:all": "echo 'Please build for all platforms manually or use CI/CD'",
    "artifacts": "napi artifacts",
    "version": "napi version",
    "postinstall": "node scripts/postinstall.js",
    "edge": "cargo run --bin edge  -- 127.0.0.1:4433 'my-secret-token' 127.0.0.1:11100",
    "supernode": "cargo run --bin supernode -- 0.0.0.0:4433 0.0.0.0:4432 ./cert/cert.pem ./cert/key.pem"
  },
  "dependencies": {
    "@napi-rs/cli": "^3.4.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "build/"
  ]
}
